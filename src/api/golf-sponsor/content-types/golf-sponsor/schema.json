{
  "kind": "collectionType",
  "collectionName": "golf_sponsors",
  "info": {
    "singularName": "golf-sponsor",
    "pluralName": "golf-sponsors",
    "displayName": "GolfSponsor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::golf-sponsor-category.golf-sponsor-category"
    },
    "name": {
      "type": "string"
    },
    "engName": {
      "type": "string"
    },
    "playerImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "birthday": {
      "type": "string"
    },
    "nationality": {
      "type": "string"
    },
    "tour": {
      "type": "string"
    },
    "debut": {
      "type": "string"
    },
    "history": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "usedClub": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "order": {
      "type": "integer"
    }
  }
}
